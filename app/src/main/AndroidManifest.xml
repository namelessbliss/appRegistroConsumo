<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.integrador3">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ResumenConsumo"></activity>
        <activity
            android:name=".SeleccionAlimento"
            android:label="Seleccione Alimentos"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".RegistroConsumo"
            android:label="Registro de Consumo"
            android:parentActivityName=".Menu"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Ejercicios" />
        <activity android:name=".CalcularRMB" />
        <activity android:name=".ControlMetabolico" />
        <activity android:name=".Actividades" />
        <activity android:name=".Perfil" />
        <activity
            android:name=".Menu"
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".CalcularIMC" />
        <activity android:name=".Registro" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AlimentosMacros"
            android:label="@string/title_Lista"
            android:parentActivityName=".ListaAlimentos"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ListaAlimentos" />
        </activity>
        <activity
            android:name=".ListaAlimentos"
            android:label="@string/title_Lista"
            android:parentActivityName=".Menu"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Menu" />
        </activity>
    </application>

</manifest>